#include <stdio.h>
int main(void)
{
    int a = 5;
    int b = 10;
    int c= 15;
    if (a > b && a > c)
        {
            printf("a = max\n");
        }
    else if (b > a && b > c) 
        {
            printf("b = max\n");
        }
     else
        {
            printf("c = max\n");
        }
   return 0;
    }
