#include <stdio.h>
int main(void)
{
int a = 5;
        switch(a)
    
    {
        case 4:
            printf("a = 4 \n");
                 break;
        case 5:
             printf ("a=5\n");
                break;
        case 6:
            printf ("a=6\n");
                break;
         default:
            printf("x is undefined \n");
                break;
    }
   return 0;
    }
