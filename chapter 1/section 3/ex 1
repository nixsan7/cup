#include <stdio.h>

int main() 
{
    int a=10; // 0b1010
//нужно получить 12 0b1100  
    int swap_bits(int num)
    {
        int bit1 = (a >> 1); // первый бит, второй считая справа налево
    int bit2 = (a >> 2); // второй бит, третий считая справа налево
    if (bit1 != bit2) 
{
    a ^= (1 << 1); //исключающее или для 1-го бита
    a ^= (1 << 2); //исключающее или для 2-го бита
}
    return a;
    }
   int result = swap_bits(a);
printf("Ответ: %d\n", result);
    return 0;
}

